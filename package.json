{
  "name": "happykit",
  "version": "2.0.1",
  "description": "happykit next refactor width rollup",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch './**/*.ts' --exec 'ts-node' examples/index.ts",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "test": "jest --coverage",
    "format": "prettier --write src/**/*.ts",
    "lint": "tslint -p tsconfig.json",
    "doc": "standard-version",
    "release": "git push --follow-tags origin master && yarn publish"
  },
  "main": "lib/happykit.cjs.js",
  "module": "lib/happykit.esm.js",
  "unpkg": "lib/happykit.umd.min.js",
  "jsdelivr": "lib/happykit.umd.min.js",
  "types": "lib/happykit.d.ts",
  "exports": {
    "require": "./lib/happykit.cjs.js",
    "import": "./lib/happykit.esm.js",
    "default": "./lib/happykit.esm.js"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "keywords": [
    "vue3js",
    "admin",
    "framework"
  ],
  "contributors": [
    {
      "name": "pumelotea",
      "email": "zf_722@yeah.net"
    }
  ],
  "bugs": {
    "url": "https://github.com/pumelotea/happykit-next/issues",
    "email": "zf_722@yeah.net"
  },
  "author": "pumelotea",
  "license": "Apache",
  "repository": {
    "type": "git",
    "url": "https://github.com/pumelotea/happykit-next.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tslint -p tsconfig.json",
      "prettier --write src/**/*.ts"
    ]
  },
  "dependencies": {
    "js-md5": "0.7.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "vue": "^3.1.2",
    "vue-router": "^4.0.3",
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/node": "^15.12.5",
    "cross-env": "^7.0.3",
    "esbuild": "^0.11.9",
    "esbuild-register": "^2.4.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "esm": "^3.2.25",
    "nodemon": "^2.0.7",
    "rollup": "^2.45.1",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^9.1.1",
    "typescript": "~4.2.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "ts-jest": "^26.4.4",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "standard-version": "^4.4.0",
    "husky": "^4.3.7",
    "canvas": "^2.7.0",
    "tslib": "^2.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
